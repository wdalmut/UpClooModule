    "description": "Zend Framework 2 Module that provides UpCloo functionality",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "upcloo",
        "related",
        "links",
        "correlate",
        "correalation",
        "semantic"
    ],
    "homepage": "http://www.upcloo.com/",
    "authors": [
